{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10"
      }
    },
    {
      "src": "requirements.txt",
      "use": "@vercel/python"
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "app.py" }],
  "buildCommand": "pip install --upgrade pip setuptools && pip install --disable-pip-version-check --target . --upgrade -r requirements.txt"
}
